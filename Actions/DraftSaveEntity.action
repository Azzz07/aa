{
    "_Type": "Action.Type.ODataService.DraftEnabled.Save",
    "Target": {
        "Service": "/aa/Services/Au.service",
        "EntitySet": "Books",
        "ReadLink": "{@odata.readLink}"
    },
    "ShowActivityIndicator": true,
    "ActionResult": {
        "_Name": "update"
    },
    "OnSuccess": {
        "Name": "/aa/Actions/UpdateEntitySuccessMessage.action",
        "Properties": {
            "Message": "Draft Saved"
        }
    },
    "OnFailure": "/aa/Actions/UpdateEntityFailureMessage.action"
}